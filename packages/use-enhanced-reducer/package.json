{
  "name": "@rest-hooks/use-enhanced-reducer",
  "version": "1.2.8",
  "description": "Add powerful orchestration to hooks-based Flux stores",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "react-native": "lib/index.js",
  "module": "lib/index.js",
  "unpkg": "dist/index.umd.min.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    ">=4.0": {
      "": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/index.d.ts"
      ]
    },
    ">=3.4": {
      "": [
        "ts3.4/index.d.ts"
      ],
      "*": [
        "ts3.4/index.d.ts"
      ]
    }
  },
  "type": "module",
  "files": [
    "src",
    "dist",
    "lib",
    "ts3.4",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:lib": "NODE_ENV=production BROWSERSLIST_ENV=2019 yarn g:babel --out-dir lib",
    "build:bundle": "yarn g:rollup && echo '{\"type\":\"commonjs\"}' > dist/package.json",
    "build:clean": "yarn g:clean",
    "build:legacy-types": "yarn g:downtypes lib ts3.4",
    "build": "run build:lib && run build:bundle",
    "dev": "run build:lib -w",
    "prepare": "run build:lib",
    "prepack": "run prepare && run build:bundle"
  },
  "keywords": [
    "react",
    "reducer",
    "flux",
    "middleware",
    "orchestration",
    "typescript",
    "redux",
    "hook",
    "state",
    "dispatch",
    "thunk",
    "state management",
    "concurrent mode",
    "async"
  ],
  "author": "Nathaniel Tucker <me@ntucker.me> (https://github.com/ntucker)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/data-client/rest-hooks/tree/master/packages/use-enhanced-reducer#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:data-client/rest-hooks.git",
    "directory": "packages/use-enhanced-reducer"
  },
  "bugs": {
    "url": "https://github.com/data-client/rest-hooks/issues"
  },
  "peerDependencies": {
    "@types/react": "^16.8.4 || ^17.0.0 || ^18.0.0-0",
    "react": "^16.8.4 || ^17.0.0 || ^18.0.0-0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@anansi/browserslist-config": "^1.4.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.30"
  }
}
