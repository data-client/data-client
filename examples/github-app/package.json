{
  "name": "github-app",
  "version": "0.4.15",
  "description": "github-app - A reactive data client example",
  "scripts": {
    "lint": "yarn g:lint src --ext .ts,.tsx",
    "format": "run lint --fix",
    "test:type": "run tsc",
    "start": "webpack serve --mode=development",
    "start:prod": "yarn dlx serve dist",
    "build:browser": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild"
  },
  "stackblitz": {
    "startCommand": "npm start"
  },
  "browserslist": "extends @anansi/browserslist-config",
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@anansi/babel-preset": "4.4.3",
    "@anansi/browserslist-config": "1.4.3",
    "@anansi/webpack-config": "16.0.7",
    "@babel/core": "7.23.3",
    "@linaria/babel-preset": "5.0.4",
    "@linaria/core": "5.0.2",
    "@linaria/react": "5.0.3",
    "@linaria/shaker": "5.0.3",
    "@types/lodash": "4.14.192",
    "@types/parse-link-header": "^2.0.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "react-refresh": "0.14.0",
    "serve": "14.2.1",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "@anansi/router": "0.9.2",
    "@ant-design/icons": "^5.0.0",
    "@data-client/graphql": "^0.9.0",
    "@data-client/hooks": "^0.1.1",
    "@data-client/react": "^0.9.0",
    "@data-client/rest": "^0.9.0",
    "@js-temporal/polyfill": "^0.4.4",
    "antd": "5.11.2",
    "parse-link-header": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-markdown": "9.0.1",
    "rehype-highlight": "7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-remove-comments": "^1.0.1",
    "uuid": "^9.0.0"
  },
  "packageManager": "yarn@3.7.0"
