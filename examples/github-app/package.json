{
  "name": "github-app",
  "version": "0.4.15",
  "description": "github-app - A reactive data client example",
  "scripts": {
    "lint": "yarn g:lint src --ext .ts,.tsx",
    "format": "run lint --fix",
    "test:type": "run tsc",
    "start": "webpack serve --mode=development",
    "start:prod": "yarn dlx serve dist",
    "build:browser": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild"
  },
  "stackblitz": {
    "startCommand": "npm start"
  },
  "browserslist": "extends @anansi/browserslist-config",
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@anansi/babel-preset": "6.0.2",
    "@anansi/browserslist-config": "1.5.1",
    "@anansi/webpack-config": "19.1.0",
    "@babel/core": "7.24.9",
    "@linaria/babel-preset": "5.0.4",
    "@linaria/core": "6.2.0",
    "@linaria/react": "6.2.1",
    "@linaria/shaker": "5.0.3",
    "@types/lodash": "4.14.192",
    "@types/parse-link-header": "^2.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "react-refresh": "0.14.2",
    "serve": "14.2.3",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4"
  },
  "dependencies": {
    "@anansi/router": "0.10.3",
    "@ant-design/icons": "^5.0.0",
    "@data-client/graphql": "^0.14.0",
    "@data-client/react": "^0.14.0",
    "@data-client/rest": "^0.14.0",
    "@js-temporal/polyfill": "^0.4.4",
    "antd": "5.19.3",
    "parse-link-header": "^2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.0.0",
    "react-markdown": "9.0.1",
    "rehype-highlight": "7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-remove-comments": "^1.0.1",
    "uuid": "^10.0.0"
  },
  "packageManager": "yarn@4.3.1"
}
