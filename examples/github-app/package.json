{
  "name": "github-app",
  "version": "0.4.10",
  "description": "github-app - A rest hooks example",
  "scripts": {
    "lint": "yarn g:lint src --ext .ts,.tsx",
    "format": "run lint --fix",
    "test:type": "run tsc",
    "start": "webpack serve --mode=development",
    "start:prod": "yarn dlx serve dist",
    "build:browser": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild"
  },
  "stackblitz": {
    "startCommand": "yarn start"
  },
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@anansi/babel-preset": "4.2.12",
    "@anansi/browserslist-config": "1.4.3",
    "@anansi/webpack-config": "15.0.30",
    "@babel/core": "7.22.9",
    "@linaria/babel-preset": "4.5.4",
    "@linaria/core": "4.5.4",
    "@linaria/react": "4.5.4",
    "@linaria/shaker": "4.5.3",
    "@types/lodash": "4.14.192",
    "@types/parse-link-header": "^2.0.0",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "react-refresh": "0.14.0",
    "serve": "14.2.0",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "@anansi/router": "0.8.1",
    "@ant-design/icons": "^5.0.0",
    "@data-client/graphql": "^0.2.0",
    "@data-client/hooks": "^0.1.1",
    "@data-client/react": "^0.2.2",
    "@data-client/rest": "^0.3.0",
    "antd": "5.7.3",
    "parse-link-header": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-markdown": "8.0.7",
    "rehype-highlight": "6.0.0",
    "remark-gfm": "^3.0.1",
    "remark-remove-comments": "^1.0.1",
    "uuid": "^9.0.0"
  },
  "packageManager": "yarn@3.6.1"
}
