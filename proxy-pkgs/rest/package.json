    "./next": {
      "types": "./next.d.ts",
      "module": "./next.js",
      "import": "./next.js",
      "require": "./next.cjs",
      "default": "./next.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.17 || ^13.7 || >=14"
  },
  "files": [
    "next.cjs",
    "next.js",
    "next.d.ts",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
