{
  "name": "@rest-hooks/rest",
  "version": "7.0.0",
  "description": "Endpoints for REST APIs",
  "homepage": "https://resthooks.io/rest",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:data-client/rest-hooks.git",
    "directory": "packages/rest"
  },
  "bugs": {
    "url": "https://github.com/data-client/rest-hooks/issues"
  },
  "keywords": [
    "REST",
    "CRUD",
    "networking",
    "fetch",
    "data",
    "cache",
    "model",
    "resource",
    "typescript",
    "path-to-regex",
    "path template",
    "data fetching",
    "data cache",
    "api",
    "normalized cache",
    "subject pattern",
    "endpoint",
    "react-native",
    "ios",
    "android",
    "web",
    "json"
  ],
  "sideEffects": false,
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "typesVersions": {
    "*": {
      "": [
        "index.d.ts"
      ],
      "next": [
        "next.d.ts"
      ],
      "*": [
        "index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "module": "./index.js",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js"
