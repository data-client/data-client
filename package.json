{
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "release": "lerna publish --no-private",
    "version": "lerna version --no-private",
    "version:breaking": "lerna version",
    "version:beta": "lerna version --no-git-tag-version --conventional-prerelease",
    "lint": "eslint --ext .ts,.tsx,.js",
    "format": "eslint --fix \"packages/*/src/**/*.{js,ts,tsx}\"",
    "clean": "lerna clean",
    "clean:types": "rimraf packages/*/*.tsbuildinfo",
    "build": "yarn build:types && lerna run build --stream",
    "build:bundle": "lerna run build:bundle --stream",
    "build:clean": "lerna run build:clean --stream",
    "build:types": "yarn build:copy:ambient && tsc --build && lerna run build:legacy-types --stream",
    "build:copy:ambient": "mkdirp ./packages/normalizr/lib && copyfiles --flat ./packages/normalizr/src/schema.d.ts ./packages/normalizr/lib/ && mkdirp ./packages/endpoint/lib && copyfiles --flat ./packages/endpoint/src/endpoint.d.ts ./packages/endpoint/lib/&& mkdirp ./packages/core/lib && copyfiles --flat ./packages/core/src/state/RIC.d.ts ./packages/core/lib/state",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "yarn test -- --ci",
    "test:coverage": "yarn test -- --coverage",
    "prepare": "yarn build:copy:ambient && tsc --build",
    "prepublishOnly": "lerna run build:legacy-types --stream"
  },
  "engines": {
    "node": "^12.17 || ^13.7 || >=14"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "devDependencies": {
    "@anansi/babel-preset": "3.2.7",
    "@anansi/browserslist-config": "1.3.3",
    "@anansi/eslint-plugin": "0.14.0",
    "@anansi/jest-preset": "0.7.11",
    "@babel/cli": "7.18.6",
    "@babel/core": "7.18.6",
    "@babel/node": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@rest-hooks/test": "^7.2.1",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "10.1.1",
    "@types/jest": "28.1.4",
    "@types/lodash": "^4.14.166",
    "@types/react": "18.0.14",
    "benchmark": "^2.1.4",
    "conventional-changelog-anansi": "^0.2.0",
    "copyfiles": "^2.4.1",
    "core-js": "3.23.3",
    "coveralls": "^3.1.0",
    "cpy-cli": "^4.0.0",
    "cross-env": "^7.0.2",
    "cross-fetch": "^3.0.6",
    "downlevel-dts": "^0.10.0",
    "eslint": "8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "ignore-styles": "^5.0.1",
    "jest": "28.1.2",
    "jest-environment-jsdom": "28.1.2",
    "lerna": "5.1.6",
    "mkdirp": "^1.0.4",
    "nock": "13.2.8",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.1",
    "react-test-renderer": "18.2.0",
    "redux": "4.2.0",
    "rimraf": "^3.0.2",
    "rollup": "2.75.7",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "10.8.2",
    "typescript": "next",
    "whatwg-fetch": "3.0.0"
  },
  "resolutions": {
    "@lerna/conventional-commits": "https://github.com/ntucker/lerna-conventional-commits.git"
  }
}
